{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InstantiateMsg",
  "type": "object",
  "required": [
    "dao",
    "denom",
    "distribution_rate",
    "min_time_elapsed_between_distributions",
    "min_time_elapsed_between_grabs",
    "owner"
  ],
  "properties": {
    "dao": {
      "type": "string"
    },
    "denom": {
      "type": "string"
    },
    "distribution_rate": {
      "type": "integer",
      "format": "uint8",
      "minimum": 0.0
    },
    "min_time_elapsed_between_distributions": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "min_time_elapsed_between_grabs": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "owner": {
      "description": "The contract's owner",
      "type": "string"
    }
  }
}
