{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "PauseInfoResponse",
  "description": "Information about if the contract is currently paused.",
  "oneOf": [
    {
      "type": "object",
      "required": [
        "Paused"
      ],
      "properties": {
        "Paused": {
          "type": "object",
          "required": [
            "until_height"
          ],
          "properties": {
            "until_height": {
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "Unpaused"
      ],
      "properties": {
        "Unpaused": {
          "type": "object"
        }
      },
      "additionalProperties": false
    }
  ]
}
