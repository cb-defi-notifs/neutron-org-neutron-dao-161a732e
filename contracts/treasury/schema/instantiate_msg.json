{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InstantiateMsg",
  "type": "object",
  "required": [
    "denom",
    "distribution_contract",
    "distribution_rate",
    "min_period",
    "owner"
  ],
  "properties": {
    "denom": {
      "type": "string"
    },
    "distribution_contract": {
      "type": "string"
    },
    "distribution_rate": {
      "type": "integer",
      "format": "uint8",
      "minimum": 0.0
    },
    "min_period": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "owner": {
      "type": "string"
    }
  }
}
